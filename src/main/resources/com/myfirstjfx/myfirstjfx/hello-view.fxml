<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.canvas.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.effect.*?>
<?import javafx.scene.layout.*?>

<VBox alignment="CENTER" prefHeight="199.0" prefWidth="195.0" spacing="20.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.myfirstjfx.myfirstjfx.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" />
   <Canvas height="200.0" width="200.0" />
    <Button fx:id="btnWelcome" defaultButton="true" onAction="#onHelloButtonClick" text="Welcome!" textAlignment="CENTER" textFill="#ab1313">
      <effect>
         <DropShadow />
      </effect></Button>
</VBox>
